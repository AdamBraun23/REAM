import java.io.*;
import java.util.*;
public class Movie 
{
  public String name;
  public String genre;
  public int rate;
  
public Movie() {
	name="";
	genre="";
	rate=0;
}

public Movie(String n, String g, int r) {
	name=n;
	genre=g;
	rate=r;
}
  
public String getName() {
	return name;
}

public String getGenre() {
  return genre;
}

public int getRate() {
  return rate;
}
}
