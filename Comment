import java.io.*;
public class Comment 
{
	public User real;
	public String com;
	public Movie movie;
	
	public Comment() {
		real=null;
		com="";
		movie = null;
	}
	
	public Comment(User r, String c, Movie m) {
		real=r;
		com=c;
		movie = m;
	}
	
	public User getUser() {
		return real;
	}
	
	public String getComment() {
		return com;
	}
	
	public Movie getMovie() {
		return movie;
	}
}
