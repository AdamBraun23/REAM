import java.io.*;
import java.util.*;
public class User 
{
  String password;
  String username;

public User() 
{
    password="";
    username="";
}
  
public User(String u, String p) 
{
	username=u;
    password=p;
}
    
public String getUsername() 
{
	return username;
}

public String getPassword() 
{
  return password;
}
}
    
